---
layout: project.njk
name: Warm Intro
---
{%- for project in projects-%}
    {%- if project.title === name -%}

                {# Content Section 1 #}
                {%- for content in project.projectContent.sectionContent-%}
                    {%- if content.sectionNum === "1" -%}
                        {%- include 'project-block-content.njk'-%}
                    {%- endif -%}
                {%- endfor -%}


                {%- for additionalContent in project.projectContent.additionalContent-%}
                        {%- if additionalContent.sectionNum <= 2-%}
                             <div class="project-block images-with-description-block container">
                                    <div class="solo-img">
                                    {%- for img in additionalContent.images-%}
                                        <img src="{{ img.formats.medium.url }}" alt="{{img.alternativeText}}" />
                                    {%- endfor -%}
                                    </div>       
                                 <div class="process-description">
                                    <h3>{{additionalContent.title}}</h3>
                                    <p>{{additionalContent.description}}</p>
                                </div>
                        </div>
                        {%- endif -%}
                {%- endfor -%}
                
                

                {# Content Section 2 #}
                {%- for content in project.projectContent.sectionContent-%}
                    {%- if content.sectionNum === "2" -%}
                        {%- include 'project-block-content.njk'-%}
                    {%- endif -%}
                {%- endfor -%}

                {%- for additionalContent in project.projectContent.additionalContent-%}
                        {%- if additionalContent.sectionNum >= 3 and additionalContent.sectionNum <=6-%}
                             <div class="project-block images-with-description-block container">
                                 <div class="process-description">
                                    <h3>{{additionalContent.title}}</h3>
                                    <p>{{additionalContent.description}}</p>   
                                </div>
                            <div class="image-grid">
                                {%- for img in additionalContent.images-%}
                                    {%- if img.formats.medium-%}
                                         <img src="{{ img.formats.medium.url }}" alt="{{img.alternativeText}}" />
                                        {%- else -%}
                                            <img src="{{ img.url }}" alt="{{img.alternativeText}}" />

                                    {%- endif -%}
                                {%- endfor -%}
                            </div>                    
                        </div>
                        {%- endif -%}           
                {%- endfor -%}



                {# Content Section 3 #}
                {%- for content in project.projectContent.sectionContent-%}
                    {%- if content.sectionNum === "3" -%}
                        {%- include 'project-block-content.njk'-%}
                    {%- endif -%}
                {%- endfor -%}

            {%- for additionalContent in project.projectContent.additionalContent-%}
                        {%- if additionalContent.sectionNum > 6 -%}
                        <div class="project-bg-color">
                             <div class="project-block images-with-description-block container">
                                 <div class="process-description">
                                    <h3>{{additionalContent.title}}</h3>
                                    <p>{{additionalContent.description}}</p>   
                                </div>
                            <div class="solo-img interactions">
                                {%- for img in additionalContent.images-%}
                                    <img src="{{ img.url }}" alt="{{img.alternativeText}}" />
                                {%- endfor -%}
                            </div>                    
                        </div>
                        </div>
                        {%- endif -%}           
                {%- endfor -%}
               
            {%- endif -%}
{%- endfor -%}